<% provide(:title, @user.username) %>


<%= render partial: 'shared/main_nav' %>
<%= render partial: 'users/profile_banner' %>
<%= render partial: 'users/message_modal' if logged_in? %>
<%= render partial: 'users/report_modal' if logged_in? %>
<%= render partial: 'users/welcome_modal' if (is_me?(@user) && params[:welcome] == "true") %>

<div class="container">
  <div class="row">
    <div class="col-lg-5 push-lg-7">
      <%= render partial: 'users/photos_card' %>
    </div>
    <div class="col-lg-7 pull-lg-5" id="profile-page-essays">
      <%= render partial: 'users/profile_about' %>
      <%= render partial: 'users/profile_occupation' %>
      <%= render partial: 'users/profile_preference' %>
    </div>
  </div>
</div>
