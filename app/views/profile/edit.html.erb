<% provide(:title, "Edit profile") %>

<%= render partial: 'shared/main_nav' %>

  <div class="container my-5">
    <div class="row">
      <div class="col-12">
        <h1 class="jumbotron-heading text-center text-md-left">Edit profile</h1>
      </div>
    </div>
  </div>

<div style="border-bottom: 1px solid #f3f3f3;">
  <div class="container py-1">
    <div class="row">
      <div class="col-12">
        <%= render partial: 'settings/nav' %>
      </div>
    </div>
  </div>
</div>

<div class="container mt-1">
  <div class="row">
    <div class="col-lg-6 mt-4">
      <%= render partial: 'profile/username' %>
      <%= render partial: 'profile/details' %>
    </div>
    <div class="col-lg-6 mt-4">
      <%= render partial: 'profile/about' %>
      <%= render partial: 'profile/occupation' %>
      <%= render partial: 'profile/preference' %>
    </div>
  </div>
</div>
