h1[data-toggle="collapse"] {
  &.collapsed {
    background-image: image-url('tune.svg');
    a {
      color: #FFF !important;
    }
  }
  background-image: image-url('close.svg');
  background-repeat: no-repeat;
  background-position: center right;
  a {
    color: rgba(255, 255, 255, 0.4) !important;
  }
}
